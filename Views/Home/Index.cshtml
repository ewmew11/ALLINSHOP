    <style>

        .carousel-cell {
            width: 190px;
            height: fit-content;
            margin-right: 30px;
            border-radius: 10px;
        }

        .carousel-nav {
            width: 25%;
            height: 450px;
            margin-right: 20px;
            border-radius: 5px;
        }

        .carousel-nav1 {
            width: 600px;
            height: 550px;
            margin-right: 30px;
            border-radius: 5px;
        }

        .carousel-image {
            display: block;
            height: 450px;
            width: 100%;
            /* set min-width, allow images to set cell width */
            min-width: 250px;
            max-width: 100%;
            margin-right: 5px;
            /* vertically center */
            top: 50%;
            transform: translateY(-50%)
        }

        .carousel.is-fullscreen .carousel-image {
            height: auto;
            max-height: 100%;
        }

        .zoom {
            padding: 5px;
            background-color: azure;
            transition: transform 0.2s;
            width: auto;
            height: auto;
            margin: auto;
        }

        .zoom:hover {
            -ms-transform: scale(1.3);
            -webkit-transform: scale(1.3);
             transform: scale(1.3);
            }

        .flickity-page-dots {
            bottom: -40px;
        }
            /* dots are lines */
        .flickity-page-dots .dot {
                height: 4px;
                width: 40px;
                margin: 0;
                border-radius: 0;
            }
        /* smaller, dark, rounded square */
        .flickity-button {
            background: #333;
        }

        .flickity-button:hover {
                background: #F90;
            }

        .flickity-prev-next-button {
            width: 30px;
            height: 30px;
            border-radius: 5px;
        }
        /* icon color */
        .flickity-button-icon {
            fill: white;
        }
        /* position outside */
        .flickity-prev-next-button.previous {
            left: -10px;
        }

        .flickity-prev-next-button.next {
            right: -40px;
        }

    </style>

</head>

